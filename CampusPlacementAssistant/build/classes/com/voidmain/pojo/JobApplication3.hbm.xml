<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.voidmain.pojo.JobApplication3" table="JOBAPPLICATION3">
        <id name="id3" type="int">
            <column name="ID3" />
            <generator class="increment" />
        </id>
        <property name="rno3" type="java.lang.String">
